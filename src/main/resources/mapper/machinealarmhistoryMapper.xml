<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.MachinealarmhistoryDao">

    <insert id="addMachinealarmhistory" parameterType="com.seer.panel.bean.Machinealarmhistory">
        INSERT INTO machinealarmhistory
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{machinealarmhistoryId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateMachinealarmhistoryById" parameterType="com.seer.panel.bean.Machinealarmhistory">
        update machinealarmhistory
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{machinealarmhistoryId}
    </update>

    <select id="selectMachinealarmhistoryList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id machinealarmhistoryId
        from machinealarmhistory
        WHERE
        1=1
        <if test="machinealarmhistoryId != null">
            and record_id = #{machinealarmhistoryId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delMachinealarmhistoryById" parameterType="java.lang.Integer">
        DELETE FROM  machinealarmhistory
        where
        record_id = #{machinealarmhistoryId}
    </delete>

</mapper>
