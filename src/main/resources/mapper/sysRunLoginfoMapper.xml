<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.SysRunLoginfoDao">

    <insert id="addSysRunLoginfo" parameterType="com.seer.panel.bean.SysRunLoginfo">
        INSERT INTO sysRunLoginfo
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{sysRunLoginfoId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateSysRunLoginfoById" parameterType="com.seer.panel.bean.SysRunLoginfo">
        update sysRunLoginfo
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{sysRunLoginfoId}
    </update>

    <select id="selectSysRunLoginfoList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id sysRunLoginfoId
        from sysRunLoginfo
        WHERE
        1=1
        <if test="sysRunLoginfoId != null">
            and record_id = #{sysRunLoginfoId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delSysRunLoginfoById" parameterType="java.lang.Integer">
        DELETE FROM  sysRunLoginfo
        where
        record_id = #{sysRunLoginfoId}
    </delete>

</mapper>
