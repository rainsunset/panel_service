<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.CutterssampleDao">

    <insert id="addCutterssample" parameterType="com.seer.panel.bean.Cutterssample">
        INSERT INTO cutterssample
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{cutterssampleId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateCutterssampleById" parameterType="com.seer.panel.bean.Cutterssample">
        update cutterssample
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{cutterssampleId}
    </update>

    <select id="selectCutterssampleList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id cutterssampleId
        from cutterssample
        WHERE
        1=1
        <if test="cutterssampleId != null">
            and record_id = #{cutterssampleId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delCutterssampleById" parameterType="java.lang.Integer">
        DELETE FROM  cutterssample
        where
        record_id = #{cutterssampleId}
    </delete>

</mapper>
