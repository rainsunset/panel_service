<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.WoundwaresampleDao">

    <insert id="addWoundwaresample" parameterType="com.seer.panel.bean.Woundwaresample">
        INSERT INTO woundwaresample
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{woundwaresampleId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateWoundwaresampleById" parameterType="com.seer.panel.bean.Woundwaresample">
        update woundwaresample
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{woundwaresampleId}
    </update>

    <select id="selectWoundwaresampleList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id woundwaresampleId
        from woundwaresample
        WHERE
        1=1
        <if test="woundwaresampleId != null">
            and record_id = #{woundwaresampleId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delWoundwaresampleById" parameterType="java.lang.Integer">
        DELETE FROM  woundwaresample
        where
        record_id = #{woundwaresampleId}
    </delete>

</mapper>
