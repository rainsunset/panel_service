<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.AcquisitioncardrunlogDao">

    <insert id="addAcquisitioncardrunlog" parameterType="com.seer.panel.bean.Acquisitioncardrunlog">
        INSERT INTO acquisitioncardrunlog
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{acquisitioncardrunlogId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateAcquisitioncardrunlogById" parameterType="com.seer.panel.bean.Acquisitioncardrunlog">
        update acquisitioncardrunlog
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{acquisitioncardrunlogId}
    </update>

    <select id="selectAcquisitioncardrunlogList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id acquisitioncardrunlogId
        from acquisitioncardrunlog
        WHERE
        1=1
        <if test="acquisitioncardrunlogId != null">
            and record_id = #{acquisitioncardrunlogId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delAcquisitioncardrunlogById" parameterType="java.lang.Integer">
        DELETE FROM  acquisitioncardrunlog
        where
        record_id = #{acquisitioncardrunlogId}
    </delete>

</mapper>
