<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.seer.panel.dao.WeiXinopenDao">

    <insert id="addWeiXinopen" parameterType="com.seer.panel.bean.WeiXinopen">
        INSERT INTO weiXinopen
        <trim prefix="(" suffix=")" suffixOverrides=",">
            record_id,
                <if test="userId != null">
                    user_id,
                </if>
                <if test="num != null">
                    num,
                </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{weiXinopenId},
                <if test="userId != null">
                    #{userId},
                </if>
                <if test="num != null">
                    #{num},
                </if>
        </trim>
    </insert>

    <update id="updateWeiXinopenById" parameterType="com.seer.panel.bean.WeiXinopen">
        update weiXinopen
        <trim prefix="set" suffixOverrides=",">
                <if test="userId != null">
                    user_id =#{userId},
                </if>
                <if test="num != null">
                    num =#{num},
                </if>
        </trim>
        where record_id = #{weiXinopenId}
    </update>

    <select id="selectWeiXinopenList" parameterType="java.util.Map"
            resultType="java.util.Map">
        select
            user_id userId,
            num num,
        record_id weiXinopenId
        from weiXinopen
        WHERE
        1=1
        <if test="weiXinopenId != null">
            and record_id = #{weiXinopenId}
        </if>
        <if test="userId != null">
            and user_id like #{userId}
        </if>
        <if test="num != null">
            and num = #{num}
        </if>
        ORDER BY record_id ASC
    </select>

    <delete id="delWeiXinopenById" parameterType="java.lang.Integer">
        DELETE FROM  weiXinopen
        where
        record_id = #{weiXinopenId}
    </delete>

</mapper>
